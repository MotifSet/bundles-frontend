{
  "name": "ethsf-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@rebass/grid": "6.0.0-4",
    "d3": "5.7.0",
    "moment": "2.22.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "react-router-redux": "4.0.8",
    "react-scripts": "2.0.4",
    "redux": "4.0.0",
    "redux-form": "7.4.2",
    "redux-thunk": "2.3.0",
    "styled-components": "3.4.9",
    "superagent": "4.0.0-beta.5"
  },
  "scripts": {
    "deploy": "react-scripts build && cd build && surge --domain https://primotif.surge.sh",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
