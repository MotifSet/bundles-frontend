  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "husky": "1.1.1",
    "lint-staged": "7.3.0",
    "prettier": "1.14.3",
    "redux-devtools": "3.4.1",
    "redux-devtools-dock-monitor": "1.1.3",
    "redux-devtools-log-monitor": "1.4.0",
    "time-series-data-generator": "0.1.4"
  }
